{
	"segments": {
		"left": [
			{
				"type": "string",
				"contents": "Fugitive",
				"highlight_groups": ["mode"]
			},
			{
				"function": "ext.segments.vim.plugin.eskk.eskk_status",
				"exclude_modes": ["nc"]
			},
			{
				"function": "ext.segments.vim.plugin.fugitive.file_directory",
				"priority": 40,
				"exclude_modes": ["nc"],
				"draw_soft_divider": false
			},
			{
				"function": "file_name",
				"draw_soft_divider": false
			},
			{
				"function": "modified_indicator",
				"before": " "
			},
			{
				"function": "ext.segments.vim.plugin.fugitive.revision",
				"priority": 20,
				"args": {
					"length": 8
				}
			},
			{
				"type": "string",
				"highlight_groups": ["background"],
				"draw_soft_divider": false,
				"draw_hard_divider": false,
				"width": "auto"
			}
		],
		"right": [
			{
				"function": "file_format",
				"exclude_modes": ["nc"],
				"priority": 50
			},
			{
				"function": "file_encoding",
				"exclude_modes": ["nc"],
				"priority": 50
			},
			{
				"function": "file_type",
				"priority": 50
			},
			{
				"type": "string",
				"name": "line_current_symbol",
				"priority": 20
			},
			{
				"function": "virtcol_current",
				"exclude_modes": ["nc"],
				"draw_soft_divider": false,
				"priority": 70,
				"after": ":"
			},
			{
				"function": "line_current",
				"draw_soft_divider": false,
				"priority": 20
			},
			{
				"function": "line_count",
				"draw_soft_divider": false,
				"priority": 20,
				"before": "/"
			}
		]
	}
}
