{
	"segments": {
		"left": [
			{
				"function": "mode",
				"exclude_modes": ["nc"]
			},
			{
				"function": "ext.segments.vim.plugin.eskk.eskk_status",
				"exclude_modes": ["nc"]
			},
			{
				"function": "paste_indicator",
				"exclude_modes": ["nc"],
				"priority": 10
			},
			{
				"function": "readonly_indicator",
				"draw_soft_divider": false,
				"after": " ",
				"args": {
					"text": "тнд"
				}
			},
			{
				"function": "file_scheme",
				"priority": 20
			},
			{
				"function": "file_directory",
				"priority": 40,
				"draw_soft_divider": false
			},
			{
				"function": "file_name",
				"draw_soft_divider": false,
				"args": {
					"display_no_file": true
				}
			},
			{
				"function": "modified_indicator",
				"before": " "
			},
			{
				"function": "branch",
				"priority": 30,
				"draw_soft_divider": false
			},
			{
				"function": "ext.segments.vim.plugin.gitgutter.hunks",
				"exclude_modes": ["nc"],
				"priority": 50,
				"before": " [",
				"after": "]",
				"args": {
					"sign_subsep": ":"
				}
			},
			{
				"type": "string",
				"highlight_groups": ["background"],
				"draw_soft_divider": false,
				"draw_hard_divider": false,
				"width": "auto"
			}
		],
		"right": [
			{
				"function": "ext.segments.vim.plugin.anzu.search_status",
				"exclude_modes": ["nc"],
				"draw_soft_divider": false,
				"priority": 10
			},
			{
				"function": "file_format",
				"priority": 60
			},
			{
				"function": "file_encoding",
				"priority": 60
			},
			{
				"function": "file_type",
				"priority": 60
			},
			{
				"type": "string",
				"name": "line_current_symbol",
				"priority": 20
			},
			{
				"function": "line_current",
				"draw_soft_divider": false,
				"priority": 20,
				"before": "[",
				"width": 3,
				"align": "r"
			},
			{
				"function": "line_count",
				"draw_soft_divider": false,
				"priority": 20,
				"before": "/",
				"after": "]",
				"width": 4,
				"align": "r"
			},
			{
				"function": "virtcol_current",
				"draw_soft_divider": false,
				"priority": 70,
				"before": ":",
				"width": 3,
				"align": "l"
			},
			{
				"function": "ext.segments.vim.plugin.watchdogs.watchdogs",
				"exclude_modes": ["nc"],
				"priority": 70
			}
		]
	}
}
